{{#if users}}
  {{#each users}}
  <section id = "profile_info" class="panel panel-default panel-heading">
    <h1 class = "title" >Name:  {{this.username}}</h1>
    <h2>Date Created: {{this.createdData}}</h2>
    <!--<h2>Number of Rockets: {{rockets.length}} </h2>-->
    <button type="button" id = "expandRocket" class = "btn btn-lg btn-default">View Profile</button><br/>
    </section>
  {{/each}}
{{/if}}
