<div class="rockets">
  {{#if rocket}}
    {{#each rocket}}
      <div class = "rocket panel panel-default">
        <div class = "panel-heading">
        <img src = "/assets/img/rocket64x64.png" alt = "rocket" class = "rocketPicture"/>
        <h1 class = "name panel-title">Name:
            {{#each this.description}}
            {{#if this.name}}
            {{this.name}}</h1>
          {{/if}}
          {{/each}}
        </div>
        <p class = "description">Description:
           <!--{{this.description.shortDesc}}</p>-->
          {{#each this.description}}
            {{#if this.shortDesc}}
              {{this.shortDesc}}
            {{/if}}
          {{/each}}
       </p>
        <p class = "dateCreated">Created On:
          <!-- {{this.description.createdData}}-->
           {{#each this.description}}
            {{#if this.createdData}}
              {{this.createdData}}
            {{/if}}
           {{/each}}
       </p>
       <div class = "panel">
       <p class = "deltaV">DeltaV:
         <!-- {{this.description.createdData}}-->
          {{#each this.statistics}}
           {{#if this.deltaV}}
             {{this.deltaV}} m/s
           {{/if}}
          {{/each}}
      </p>
      <p class = "mass">Mass:
        <!-- {{this.description.createdData}}-->
         {{#each this.statistics}}
          {{#if this.mass}}
            {{this.mass}} kg
          {{/if}}
         {{/each}}
     </p>
     <p class = "cost">Cost: $
       <!-- {{this.description.createdData}}-->
        {{#each this.statistics}}
         {{#if this.cost}}
           {{this.cost}}
         {{/if}}
        {{/each}}
    </p>
    <p class = "stages">Stages:
      <!-- {{this.description.createdData}}-->
       {{#each this.statistics}}
        {{#if this.stages}}
          {{this.stages}}
        {{/if}}
       {{/each}}
   </p>
    </div>
      </div>
    {{/each}}
  {{else}}
      <h3 class = "emptyRockets">An Error has Occured</h3>
  {{/if}}
</div
